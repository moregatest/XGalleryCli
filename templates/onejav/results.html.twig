{% extends "base/base.html.twig" %}
{% block title %}XGallery - Onejav - {{ keyword }}{% endblock %}
{% block body %}
    <div class="container-fluid">
        <div class="row">
            <div class="{% if items|length == 1 %}card{% else %}card-columns{% endif %}">
                {% for key, result in items %}
                    {% set item = result[0] %}
                    {% set downloads = {} %}
                    {% set downloads = downloads|merge(result) %}
                    {{ include('/onejav/card.html.twig') }}
                {% endfor %}
            </div>
        </div>
    </div>
{% endblock %}

